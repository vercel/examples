#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/dist/cli/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/dist/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/dist/cli/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/dist/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules/nitropack/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/nitropack@2.12.4_@netlify+blobs@9.1.2/node_modules:/Users/anthonyshew/projects/open/examples/framework-boilerplates/nitro-plugins/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli/index.mjs" "$@"
else
  exec node  "$basedir/../../dist/cli/index.mjs" "$@"
fi
