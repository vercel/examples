{
  "name": "nextjs-builder-io-landing-page",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/personalization-utils": "0.0.7",
    "@builder.io/react": "^1.1.42",
    "@builder.io/utils": "^1.0.4",
    "@szhsin/react-menu": "^2.1.0",
    "@types/js-cookie": "^3.0.0",
    "@vercel/fetch": "^6.1.0",
    "atob": "^2.1.2",
    "jest": "^26.6.3",
    "js-cookie": "^3.0.1",
    "next": "latest",
    "next-seo": "^4.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-merge-refs": "^1.1.0",
    "react-tiny-fab": "^4.0.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "prettier": "^2.1.2",
    "typescript": "^4.3.2",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
