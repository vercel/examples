{
  "name": "examples",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "22.x",
    "pnpm": "9.13.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "new-example": "plop example",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "!((**/.vercel/**/*)|(**/*tanstack-start/**/*))*.{ts,tsx}": [
      "prettier --write",
      "eslint -c .eslintrc.json --no-eslintrc --no-ignore --max-warnings=0 --fix"
    ],
    "**/*tanstack-start/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-next": "canary",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@9.13.0",
  "dependencies": {
    "@vercel/sdk": "^1.18.5"
  }
}
