{
  "name": "vercel-examples",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --write .",
    "update": "node update-dep.mjs",
    "update-template": "node scripts/update-template.mjs",
    "update-all-templates": "node scripts/update-all-templates.mjs",
    "new-example": "plop example",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "pre-commit": "lint-staged",
  "devDependencies": {
    "@vercel/fetch": "^6.2.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "plop": "^3.0.5",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --max-warnings=0 --fix"
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write"
    ]
  }
}
